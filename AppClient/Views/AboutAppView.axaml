<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:res="using:AppClient.Properties"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AppClient.Views.AboutAppView">
	<DockPanel>
		<WrapPanel HorizontalAlignment="Right" DockPanel.Dock="Bottom">
			<Button Command="{Binding Close}" Content="{x:Static res:Resources.DialogWindowClose}"></Button>
		</WrapPanel>
		<StackPanel DockPanel.Dock="Left"></StackPanel>
	</DockPanel>
</UserControl>
